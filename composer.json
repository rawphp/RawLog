{
    "name": "rawphp/raw-log",
    "type": "library",
    "description": "RawLog is a wrapper around Monolog and is used by RawPHP framework and other applications.",
    "keywords": [ "log" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "rawphp/raw-mail": "0.*@dev",
        "rawphp/raw-support": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
		"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawLog\\": "src/RawPHP/RawLog/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
